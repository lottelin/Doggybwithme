<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <a class="navbar-brand" href="#">
    <%= image_tag 'Logo_Dog.png', class: 'logo' %>
  </a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

<div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item dropdown">
      <%= image_tag 'bone.png', class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
          <%= link_to "Sign In", new_user_session_path, class: 'dropdown-item' %>
          <%= link_to "Log Out", destroy_user_session_path, method: :delete, class: 'dropdown-item'  %>
          <%= link_to "Rent Your Doggy", new_dog_path, class: 'dropdown-item'  %>
          <%= link_to "Discover Doggies", dogs_path, class: 'dropdown-item'  %>
          <%= link_to "Profile", class: 'dropdown-item'  %>
        </div>
      </li>
    </ul>
  </div>
</div>
